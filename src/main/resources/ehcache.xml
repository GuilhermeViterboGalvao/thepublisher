<?xml version="1.0" encoding="UTF-8"?>
<ehcache 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:noNamespaceSchemaLocation="ehcache.xsd">
    <diskStore path="java.io.tmpdir"/>
    <cacheManagerEventListenerFactory class="" properties=""/>    
	<defaultCache
    	maxElementsInMemory="100"
    	timeToLiveSeconds="120"        
        timeToIdleSeconds="120"        
        overflowToDisk="false"
        diskPersistent="false"
        eternal="false"
        diskExpiryThreadIntervalSeconds="120"
        memoryStoreEvictionPolicy="LRU">
    </defaultCache>
    <cache 
        name="pageCache"
        maxElementsInMemory="500"
        timeToLiveSeconds="1800"        
        timeToIdleSeconds="1800"        
        overflowToDisk="false"
        eternal="false"
        memoryStoreEvictionPolicy="LFU">
        <searchable/>      
	</cache>    
    <cache 
        name="tile"
        maxElementsInMemory="100"
        timeToLiveSeconds="600"        
        timeToIdleSeconds="300"        
        overflowToDisk="false"
        diskPersistent="false"
        eternal="false"
        memoryStoreEvictionPolicy="LFU">
    	<searchable>
			<searchAttribute name="xml" class="com.publisher.ehcache.search.XMLAttributeExtractor"/>		
		</searchable>	               
	</cache>
    <cache
        name="permanentLinks"
        maxElementsInMemory="1000"        
        overflowToDisk="false"
        eternal="true"
        memoryStoreEvictionPolicy="LFU">
    </cache>
    <cache
        name="com.publisher.entity.Account"
        maxElementsInMemory="20"        
        timeToLiveSeconds="120"
        timeToIdleSeconds="120"        
        overflowToDisk="false"
        eternal="false"
        memoryStoreEvictionPolicy="LFU">   
    </cache>
    <cache 
        name="com.publisher.entity.Article"
        maxElementsInMemory="1000"
        timeToLiveSeconds="120"
        timeToIdleSeconds="60"        
        overflowToDisk="false"
        eternal="false"
        memoryStoreEvictionPolicy="LFU">
    </cache>
    <cache
        name="com.publisher.entity.Category"
        maxElementsInMemory="250"        
        timeToLiveSeconds="120"
        timeToIdleSeconds="120"        
        overflowToDisk="false"
        eternal="false"
        memoryStoreEvictionPolicy="LFU">   
    </cache>
    <cache
        name="com.publisher.entity.Page"
        maxElementsInMemory="100"
        timeToLiveSeconds="120"
        timeToIdleSeconds="120"        
        overflowToDisk="false"
        eternal="false"
        memoryStoreEvictionPolicy="LFU">   
    </cache>
    <cache
        name="com.publisher.entity.PermanentLink"
        maxElementsInMemory="1000"
        timeToLiveSeconds="120"
        timeToIdleSeconds="120"        
        overflowToDisk="false"
        eternal="false"
        memoryStoreEvictionPolicy="LFU">   
    </cache>
    <cache
        name="com.publisher.entity.Photo"
        maxElementsInMemory="2000"
        timeToLiveSeconds="10"
        timeToIdleSeconds="1800"        
        overflowToDisk="false"
        eternal="false"
        memoryStoreEvictionPolicy="LFU">   
    </cache>
    <cache
        name="com.publisher.entity.PhotoGallery"
        maxElementsInMemory="2000"
        timeToLiveSeconds="120"
        timeToIdleSeconds="120"        
        overflowToDisk="false"
        eternal="false"
        memoryStoreEvictionPolicy="LFU">   
    </cache>
    <cache
        name="com.publisher.entity.Skin"
        maxElementsInMemory="100"
        timeToLiveSeconds="120"
        timeToIdleSeconds="120"        
        overflowToDisk="false"
        eternal="false"
        memoryStoreEvictionPolicy="LFU">   
    </cache>
    <cache
        name="userPollVote"
        maxElementsInMemory="100"
        timeToLiveSeconds="600"        
        timeToIdleSeconds="600"
        overflowToDisk="false"
        eternal="true"
        memoryStoreEvictionPolicy="LFU">      
    </cache>              
</ehcache>